# Ignore OS generated files
.DS_Store
Thumbs.db

# Ignore node_modules (if you're using Node.js or front-end tools)
node_modules/

# Ignore Composer's vendor directory (for Composer-managed PHP dependencies)
vendor/

# Ignore composer.lock (if you don't want to track it)
composer.lock

# Ignore Composer's executable file (if included in your project)
composer.phar

# Ignore environment configuration files
.env

# Ignore IDE/editor-specific files
.idea/
.vscode/

# Ignore temporary files and backup files from text editors
*.swp
*.swo
*.bak
*.tmp

# Ignore compiled files (like for SASS/LESS or JS source maps)
*.css.map
*.js.map

# Ignore WordPress auto-generated files in wp-content
wp-content/plugins/
wp-content/themes/

# Ignore all files inside the theme's build or dist folder (if applicable)
build/
dist/

# Ignore test or local development files (e.g., browser dev tools)
*.log
*.pid

.wp-env.json
